@*
@model List<ObjetoEscuelaBase>
*@
<table class= "table table-dark table-hover table-sm">
    <head>
        <tr>
            <th scope="col">Nombre </th>
            <th scope="col">Id</th>
        </tr>
    </head>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Nombre</td>
                <td>@obj.UniqueId</td>
            </tr>
        }
    </tbody>
</table>