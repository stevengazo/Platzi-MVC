@model List<Alumno>

@{
    ViewData["Title"]= "Informaci√≥n Alumnos";
    Layout="Simple";
}


<h1>  @ViewData["Title"]</h1>

<strong>Vista Parcial</strong>


<table class= "table table-dark table-hover table-sm">
    <head>
        <tr>
            <th scope="col">Nombre </th>
            <th scope="col">Id</th>
        </tr>
    </head>
    <tbody>
        @foreach (var alumno in Model)
        {
            <tr>
                <td>@alumno.Nombre</td>
                <td>@alumno.UniqueId</td>
            </tr>
        }
    </tbody>
</table>

<p> Server time: <i>@ViewBag.Fecha</i></p>